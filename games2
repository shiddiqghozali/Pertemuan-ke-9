#include <iostream>
using namespace std;

int main()
{
    int a;
    int b;
    int n;

    cout<<"Tampilkan angka bertingkat"<<endl;
    cout<<"nilai awal = ";
    cin>>a;
    cout<<"nilai akhir = ";
    cin>>n;

    for(a=0; a<n; a++){
        for(b=0; b<a; b++){
            cout<<"*";
        }  cout<<endl;
    }
    return 0;
}
